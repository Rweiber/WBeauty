    <section class="carrossel">
      <h2 class="carrossel__titulo">Lançamentos</h2>
      <div class="swiper">
        <!-- Additional required wrapper -->
        <div class="swiper-wrapper">
          <!-- Slides -->
          <div class="swiper-slide">
            <img src="/images/produtos/perf1.png" alt="foto do perfumes 1" />
            <div class="product-info">
              <h3 class="product-name">Rosé Dawn</h3>
              <p class="product-price">
                <span class="old-price">R$179,90</span>
                <span class="new-price">R$109,90</span>
              </p>
            </div>
          </div>
          <div class="swiper-slide">
            <img src="/images/produtos/perf2.png" alt="foto do perfume 2" />
            <div class="product-info">
              <h3 class="product-name">Serene Ocean</h3>
              <p class="product-price">
                <span class="old-price">R$149,90</span>
                <span class="new-price">R$99,90</span>
              </p>
            </div>
          </div>
          <div class="swiper-slide">
            <img src="/images/produtos/perf3.png" alt="foto do perfume 3" />
            <div class="product-info">
              <h3 class="product-name">Scarlet Elegance</h3>
              <p class="product-price">
                <span class="old-price">R$199,90</span>
                <span class="new-price">R$149,90</span>
              </p>
            </div>
          </div>

          ...
        </div>
        <!-- If we need pagination -->
        <div class="swiper-pagination"></div>

        <!-- If we need navigation buttons -->
        <div class="swiper-button-prev"></div>
        <div class="swiper-button-next"></div>

        <!-- If we need scrollbar -->
        <div class="swiper-scrollbar"></div>
      </div>
    </section>
